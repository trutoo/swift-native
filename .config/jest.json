{
  "preset": "react-native",
  "moduleFileExtensions": [
    "js",
    "ts",
    "tsx"
  ],
  "rootDir": "../",
  "transform": {
    "^.+\\.(js)$": "babel-jest",
    "\\.(ts|tsx)$": "ts-jest"
  },
  "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(js|ts|tsx)$",
  "testPathIgnorePatterns": [
    "\\.snap$",
    "node_modules"
  ],
  "cacheDirectory": ".cache"
}
